syntax "JSON" "\.json$"
header "^\{$"
# You can't add a comment to JSON.
comment ""

color brightblue   "\<[-]?[1-9][0-9]*([Ee][+-]?[0-9]+)?\>"  "\<[-]?[0](\.[0-9]+)?\>"
color cyan  "\<null\>"
color brightcyan "\<(true|false)\>"
color brightyellow ""(\\.|[^"])*"|'(\\.|[^'])*'"
color brightgreen "\"(\\"|[^"])*\"[[:space:]]*:"  "'(\'|[^'])*'[[:space:]]*:"
color brightmagenta   "\\u[0-9a-fA-F]{4}|\\[bfnrt'"/\\]"
color ,green "[[:space:]]+$"
color ,red "    + +| +  +"
